#!/bin/bash

# SETUP
c=$PODDY/cmd/{1:-install}

# WORKFLOWS
$c/workflow acestep-1.5-prompt-lora-blending
$c/workflow audio_ace_step_1_5_cover
$c/workflow audio_ace_step_1_5_cover_semantic_blending
$c/workflow audio_ace_step_1_5_edit

# NODES
$c/node https://github.com/Kosinkadink/ComfyUI-VideoHelperSuite
$c/node https://github.com/ryanontheinside/ComfyUI_RyanOnTheInside

# MODELS
$c/model ace_step_1.5_turbo_aio.safetensors diffusion_models https://huggingface.co/Comfy-Org/ace_step_1.5_ComfyUI_files/resolve/main/checkpoints/ace_step_1.5_turbo_aio.safetensors
